---
layout: none
---
[
{% for element in site.pages %}{% comment %}
{% endcomment %}{% unless element.url == "/search-map.json" or element.url == "/404.html" %}{% comment %}
{% endcomment %}  {
    "title": "{% if element.url == "/index.html" %}Home{% else %}{{ element.title }}{% endif %}",
    "content": "{{ element.content | strip_html | strip_newlines | remove: "	" | remove: "\"" }}",
    "category": "{{ element.category }}",
    "tags": "{% for tag in element.tags %}{{ tag }}{% unless forloop.last %}, {% endunless %}{% endfor %}",
    "url": "{{ site.baseurl }}{{ element.url | remove: "index.html" }}",
    "date": "{{ element.date }}"
 },
{% endunless %}{% comment %}
{% endcomment %}{% endfor %}{}
]
